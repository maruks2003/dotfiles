#!/usr/bin/bash
# simple script to launch dmenu with colors from wal

# Import the colors
# shellcheck source=/home/morpheus/.cache/wal/colors.sh
. "${HOME}/.cache/wal/colors.sh"

passmenu -fn "-misc-hermit-bold-i-normal--17-120-100-*-m-0-iso10646-*" -nb "$color0" -nf "$color15" -sb "$color1" -sf "$color15"
